        map.addLayer({ //this is the way to add geojson layer to show up
            "id": "loc-df",
            "minzoom": 0,
            "maxzoom": 22,
            "type": "circle",
            "paint": {
                "circle-color": "hsl(24, 69%, 31%)"
            },
            "layout": {},
            "source": "loc-df",
        });